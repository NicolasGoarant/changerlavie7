<div class="paddingbottom">
<div class="fondcontainer-ter fixed-bottom col-sm-7 col-centered-md-6 col-centered-lg-6">
  <div class="row-fluid">
      <div class="col footer d-flex justify-content-between">
          <%= link_to newspapers_path do %>
          <%= image_tag "ccnewspaper.png", class: "logobottom" %>
          <p>à la une</p>
          <% end %>

          <%= link_to library_path(current_user.library) do %>
          <%= image_tag "ccarticlelogo.png", class: "logobottom" %>
          <p>sélection</p>
          <% end %>

          <%= link_to newspaper_path(1) do %>
          <%= image_tag "ccmap.png", class: "logobottom" %>
          <p>la carte</p>
          <% end %>

          <%= link_to archive_path(1) do %>
          <%= image_tag "cclibrary.png", class: "logobottom" %>
          <p>archives</p>
          <% end %>

      </div>
    </div>
  </div>
</div>



